import libc

/**
 * Itself refers to an unsigned integer. This doesn't provide any performance
 * benefits or such; most functions also don't take `UInt` as a parameter so
 * use don't when you're sure.
 */
public class UInt: Number<UInt> {
    public static func **(lhs: UInt, rhs: UInt) -> T llvm {
        return pow(base: lhs as Double, power: rhs as Double)
    }
}