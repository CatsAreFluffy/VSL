{
  "name": "node-vsl",
  "version": "0.0.1",
  "description": "VSL written in Node",
  "main": "index.js",
  "scripts": {
    "build": "npm run nearley && NODE_ENV=production babel src/ -d lib/ -s",
    "watch": "babel src/ -d lib/ -s --watch",
    "nearley": "nearleyc src/vsl/parser/parser.ne -o lib/vsl/parser/parser.js",
    "docs": "esdoc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsl-land/node-VSL.git"
  },
  "author": "somebody1234",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vsl-land/node-VSL/issues"
  },
  "homepage": "https://github.com/vsl-land/node-VSL#readme",
  "dependencies": {
    "fs-promise": "^2.0.2",
    "nearley": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-loop-optimizer": "^1.2.3",
    "babel-preset-flow": "^6.23.0",
    "esdoc": "^0.5.2"
  }
}
