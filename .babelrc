    "plugins": [
        "include",
        "loop-optimizer",
        "wildcard",
        "add-module-exports",
        "transform-runtime",
        "transform-class-properties",
        "transform-do-expressions",
        "transform-function-bind"
    ],
    "presets": [
        "flow",
        ["env", {
            "targets": {
                "node": 4.2
            }
        }],
    ],
    "ignore": [
        "vsl/interpreter/integer_asm.js"
    ]
}